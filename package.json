    "@nuxt/test-utils": "^3.14.2",
    "@nuxt/ui": "^2.18.4",
    "@nuxtjs/i18n": "^8.5.3",
    "@vitest/coverage-v8": "^2.1.1",
    "@vitest/ui": "^2.1.1",
    "@vue/test-utils": "^2.4.6",
    "better-sqlite3": "^11.3.0",
    "drizzle-kit": "^0.24.2",
    "happy-dom": "^15.7.4",
    "nuxi": "^3.13.2",
    "nuxt": "^3.13.2",
    "nuxt-auth-utils": "^0.3.8",
    "playwright-core": "^1.47.1",
    "vitest": "2.1.1",
    "vue-tsc": "^2.1.6",
    "wrangler": "3.78.2"
